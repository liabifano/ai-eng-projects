      "outputs": [],
      "source": [
        "tests = [\"Once upon a time\", \"What is 2+2?\", \"Suggest a party theme.\", \"Eu sou\", \"My name\"]\n",
        "\n",
        "for prompt in tests:\n",
        "  for model in MODELS.keys():\n",
        "    for tokeniz in TOKENIZERS.keys():\n",
        "      generate(model, tokeniz, prompt, \"top-k\", 80)\n"
      ]
